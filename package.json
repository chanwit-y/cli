{
  "name": "@chanwity/vega",
  "version": "0.0.14",
  "description": "A CLI tool to convert SVG icons to TypeScript React components",
  "main": "index.ts",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "vega": "./packages/cli/index.ts"
  },
  "scripts": {
    "api": "cd packages/app/api && bun run dev",
    "cli": "cd packages/cli && bun run index.ts",
    "serve": "cd packages/app/web && bun run dev",
    "build:ui": "cd packages/ui && bun run build",
    "serve:all": "bun run build:ui && bun run serve",
    "build": "cd packages/app/web && bun run build"
  },
  "keywords": ["svg", "tsx", "react", "icons", "cli", "typescript", "components"],
  "author": "zenith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vega.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "files": [
    "packages/cli/",
    "packages/cli/template/*",
    "packages/app/web/out/",
    "packages/app/web/dist/",
    "README.md"
  ],
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/router-plugin": "^1.130.15",
    "@types/bun": "latest",
    "@types/dom-parser": "^0.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/xmldom": "^0.1.34",
    "tailwindcss": "^4.1.11"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@hookform/resolvers": "^5.2.1",
    "@inquirer/prompts": "^7.8.0",
    "@radix-ui/themes": "^3.2.1",
    "@sinclair/typebox": "^0.34.40",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "axios": "^1.11.0",
    "chalk": "^5.4.1",
    "clsx": "^2.1.1",
    "elysia": "^0.8.17",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ora": "^8.2.0",
    "react-hook-form": "^7.62.0",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "xmldom": "^0.6.0",
    "zod": "^4.0.15"
  }
}
