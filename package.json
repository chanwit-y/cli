{
  "name": "@chanwity/vega",
  "version": "0.0.14",
  "description": "A CLI tool to convert SVG icons to TypeScript React components",
  "main": "index.ts",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "vega": "./packages/cli/index.ts"
  },
  "scripts": {
    "cli": "cd packages/cli && bun run index.ts",
    "serve": "cd packages/app/web && bun run dev"
  },
  "keywords": ["svg", "tsx", "react", "icons", "cli", "typescript", "components"],
  "author": "zenith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vega.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "files": [
    "packages/cli/",
    "packages/cli/template/*",
    "packages/app/web/out/",
    "packages/app/web/dist/",
    "README.md"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dom-parser": "^0.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/xmldom": "^0.1.34",
    "tailwindcss": "^4.1.11"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.0",
    "@radix-ui/themes": "^3.2.1",
    "chalk": "^5.4.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ora": "^8.2.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "xmldom": "^0.6.0"
  }
}
